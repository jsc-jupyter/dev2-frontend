                this.tryCount--;
                if (this.tryCount >= 0) {
                  $.ajax(this);
                  return;
                }
                showToast("Request to Server failed. Try refreshing website");
                console.error("API Request failed:", textStatus, errorThrown);
                return;
              }
              if (jqXHR.status == 403) {                
